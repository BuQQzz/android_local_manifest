<?xml version="1.0" encoding="UTF-8"?>
  <manifest>

    <remote name="github" fetch="git://github.com/" />
    <remote name="omapzoom" fetch="git://git.omapzoom.org/" />

    <!-- AOSP removals -->
    <remove-project name="platform/build" />
    <remove-project name="platform/hardware/libhardware" />
    <remove-project name="platform/hardware/libhardware_legacy" />
    <remove-project name="platform/hardware/ti/omap4xxx" />
    <remove-project name="platform/hardware/ti/wlan" />
    <remove-project name="platform/system/core" />
    <remove-project name="platform/system/vold" />

    <!-- replacements -->
    <project path="build" name="KFire-Android/android_build" remote="github" revision="jb-aosp" >
        <copyfile src="core/root.mk" dest="Makefile" />
    </project>

    <!-- OMAPzoom projects -->
    <project path="external/wpa_supplicant_8_ti" name="platform/external/wpa_supplicant_8_ti" remote="omapzoom" revision="jb-release">
        <annotation name="vendor" value="ti" />
    </project>
    <project path="device/ti/common-open" name="device/ti/common-open" remote="omapzoom" revision="jb-release" />
    <project path="device/ti/proprietary-open" name="device/ti/proprietary-open" remote="omapzoom" revision="jb-release" />
    <project path="hardware/ti/domx" name="platform/hardware/ti/domx" remote="omapzoom" revision="jb-release" />
    <project path="hardware/ti/omap4xxx" name="platform/hardware/ti/omap4xxx" remote="omapzoom" revision="p-jb-release" />
    <project path="hardware/ti/wlan" name="platform/hardware/ti/wlan" remote="omapzoom" revision="p-jb-release" />

    <!-- CM projects -->
    <project path="external/busybox" name="CyanogenMod/android_external_busybox" remote="github" revision="jellybean" />
    <project path="hardware/libhardware" name="KFire-Android/android_hardware_libhardware" remote="github" revision="jellybean" />
    <project path="hardware/libhardware_legacy" name="KFire-Android/android_hardware_libhardware_legacy" remote="github" revision="jellybean" />
    <project path="packages/apps/Superuser" name="CyanogenMod/android_packages_apps_Superuser" remote="github" revision="jellybean" />
    <project path="system/vold" name="CyanogenMod/android_system_vold" remote="github" revision="jellybean" />

    <!-- modified CM projects -->
    <project path="system/core" name="KFire-Android/android_system_core" remote="github" revision="jellybean" />

    <!-- local projects -->
    <project path="device/amazon/otter" name="KFire-Android/device-amazon-otter" remote="github" revision="jb-dev" />
    <project path="kernel/amazon/otter" name="KFire-Android/kernel_omap" remote="github" revision="p-android-omap-3.0" />
    <project path="vendor/amazon/otter" name="KFire-Android/android_vendor_amazon_otter" remote="github" revision="jb-dev" />

  </manifest>

